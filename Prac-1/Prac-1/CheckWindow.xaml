        <Label Content="0" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="363,184,0,0" FontFamily="Yu Gothic UI" FontSize="24" Foreground="White" Background="#00000000" FontWeight="Bold" RenderTransformOrigin="0.304,0.619"/>
        <TextBox HorizontalAlignment="Left" Height="35" TextWrapping="Wrap" VerticalAlignment="Top" Width="289" Margin="42,231,0,0" BorderBrush="{x:Null}" Text="" FontFamily="Yu Gothic UI" FontSize="24" Foreground="White" Background="#FF00619C" SelectionBrush="#FF38C0FF" FontWeight="Bold"/>
        <Label Content="Статичний аналіз " HorizontalAlignment="Left" VerticalAlignment="Top" Margin="578,95,0,0" FontFamily="Yu Gothic UI" FontSize="24" Foreground="White" FontWeight="Bold"/>
        <Label Content="Дисперсії вибірок: " HorizontalAlignment="Left" VerticalAlignment="Top" Margin="503,137,0,0" FontFamily="Yu Gothic UI" FontSize="24" Foreground="White"/>
        <Label Content="P ідентифікація: " HorizontalAlignment="Left" VerticalAlignment="Top" Margin="503,179,0,0" FontFamily="Yu Gothic UI" FontSize="24" Foreground="White"/>
        <Label Content="Помилка 1-го роду: " HorizontalAlignment="Left" VerticalAlignment="Top" Margin="503,221,0,0" FontFamily="Yu Gothic UI" FontSize="24" Foreground="White"/>
        <Label Content="Помилка 2-го роду: " HorizontalAlignment="Left" VerticalAlignment="Top" Margin="503,263,0,0" FontFamily="Yu Gothic UI" FontSize="24" Foreground="White"/>
        <ComboBox x:Name="CountAttempts" HorizontalAlignment="Left" VerticalAlignment="Top" Width="46" Margin="188,142,0,0" Height="37" Background="#FF00A682" FontFamily="Yu Gothic UI" FontSize="22" Opacity="0.8">
            <ListBoxItem Content="3" FontSize="15" FontFamily="Yu Gothic UI"/>
            <ListBoxItem Content="4" FontSize="15" FontFamily="Yu Gothic UI"/>
            <ListBoxItem Content="5" FontSize="15" FontFamily="Yu Gothic UI"/>
            <ListBoxItem Content="6" FontSize="15" FontFamily="Yu Gothic UI"/>
            <ListBoxItem Content="7" FontSize="15" FontFamily="Yu Gothic UI"/>
